{
	"name": "vscode-hyperupcall",
	"displayName": "hyperupcall",
	"description": "Tools picked from various extensions",
	"version": "0.12.3",
	"publisher": "EdwinKofler",
	"engines": {
		"vscode": "^1.12.0"
	},
	"license": "BSD-3-Clause",
	"repository": {
		"type": "git",
		"url": "https://github.com/hyperupcall/hyperupcall"
	},
	"bugs": {
		"url": "https://github.com/hyperupcall/hyperupcall/issues"
	},
	"author": {
		"name": "Edwin Kofler",
		"url": "https://edwinkofler.com"
	},
	"categories": [
		"Snippets",
		"Programming Languages"
	],
	"keywords": [
		"Linux",
		"Systemd",
		"Configurations",
		"panda",
		"utils"
	],
	"icon": "images/icon.jpg",
	"contributes": {
		"languages": [
			{
				"id": "awk",
				"aliases": [
					"AWK",
					"awk"
				],
				"extensions": [
					".awk",
					".gawk"
				],
				"configuration": "./language-configuration/awk.language-configuration.json"
			},
			{
				"id": "desktop",
				"aliases": [
					"Desktop",
					"desktop"
				],
				"firstLine": "^\\[Desktop Entry\\]",
				"extensions": [
					".desktop",
					".theme"
				],
				"configuration": "./language-configuration/desktop.language-configuration.json"
			},
			{
				"id": "dircolors",
				"aliases": [
					"dir_colors"
				],
				"extensions": [
					".dircolors",
					".dir_colors"
				],
				"configuration": "./language-configuration/dircolors.language-configuration.json"
			},
			{
				"id": "env",
				"aliases": [
					"Environment Variables",
					"dotenv"
				],
				"extensions": [
					".env",
					".env.sample",
					".env.example"
				],
				"configuration": "./language-configuration/env.language-configuration.json"
			},
			{
				"id": "i3",
				"aliases": [
					"i3_config"
				],
				"extensions": [
					".i3.config"
				],
				"configuration": "./language-configuration/i3.language-configuration.json"
			},
			{
				"id": "json5",
				"aliases": [
					"JSON5",
					"json5"
				],
				"extensions": [
					".json5"
				],
				"configuration": "./language-configuration/i3.language-configuration.json"
			},
			{
				"id": "ninja",
				"aliases": [
					"Ninja"
				],
				"extensions": [
					".ninja"
				],
				"configuration": "./language-configuration/ninja.language-configuration.json"
			},
			{
				"id": "po",
				"aliases": [
					"gettext",
					"po"
				],
				"extensions": [
					".po",
					".pot",
					".potx"
				],
				"configuration": "./language-configuration/po.language-configuration.json"
			},
			{
				"id": "sxhkdrc",
				"aliases": [
					"sxhkdrc"
				],
				"extensions": [
					".sxhkdrc.conf"
				],
				"configuration": "./language-configuration/sxhkdrc.language-configuration.json"
			},
			{
				"id": "systemd-conf",
				"aliases": [
					"Systemd Configuration",
					"systemd-conf",
					"systemd-unit-file"
				],
				"extensions": [
					".link",
					".netdev",
					".network",
					".service",
					".socket",
					".device",
					".mount",
					".automount",
					".swap",
					".target",
					".path",
					".timer",
					".snapshot",
					".slice",
					".scope"
				],
				"configuration": "./language-configuration/systemd.language-configuration.json"
			},
			{
				"id": "terminfo",
				"aliases": [
					"termcap"
				],
				"extensions": [
					".termcap",
					".src"
				],
				"configuration": "./language-configuration/terminfo.language-configuration.json"
			},
			{
				"id": "xdefaults",
				"aliases": [
					"Xdefaults",
					"xdefaults"
				],
				"extensions": [
					".Xresources",
					".xresources",
					".Xdefaults",
					".xdefaults"
				],
				"configuration": "./language-configuration/xresources.language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "desktop",
				"scopeName": "source.desktop",
				"path": "./syntaxes/desktop.tmLanguage.json"
			},
			{
				"language": "dircolors",
				"scopeName": "source.dircolors",
				"path": "./syntaxes/dircolors.tmLanguage.json"
			},
			{
				"language": "env",
				"scopeName": "source.env",
				"path": "./syntaxes/env.tmLanguage.json"
			},
			{
				"language": "i3",
				"scopeName": "source.i3",
				"path": "./syntaxes/i3.tmLanguage.json"
			},
			{
				"language": "json5",
				"scopeName": "source.json5",
				"path": "./syntaxes/json5.tmLanguage.json"
			},
			{
				"language": "ninja",
				"scopeName": "source.ninja",
				"path": "./syntaxes/ninja.tmLanguage.json"
			},
			{
				"language": "po",
				"scopeName": "source.po",
				"path": "./syntaxes/po.tmLanguage.json"
			},
			{
				"language": "sxhkdrc",
				"scopeName": "source.sxhkdrc",
				"path": "./syntaxes/sxhkdrc.tmLanguage.json"
			},
			{
				"language": "systemd-conf",
				"scopeName": "source.systemd",
				"path": "./syntaxes/systemd.tmLanguage.json"
			},
			{
				"language": "terminfo",
				"scopeName": "source.terminfo",
				"path": "./syntaxes/terminfo.tmLanguage.json"
			},
			{
				"language": "xdefaults",
				"scopeName": "source.xdefaults",
				"path": "./syntaxes/xdefaults.tmLanguage.json"
			},
			{
				"language": "awk",
				"scopeName": "source.awk",
				"path": "./syntaxes/awk.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"path": "./snippets/basalt.code-snippets"
			},
			{
				"language": "desktop",
				"path": "./snippets/desktop.code-snippets"
			},
			{
				"language": "editorconfig",
				"path": "./snippets/editorconfig.code-snippets"
			},
			{
				"language": "json",
				"path": "./snippets/eslint.code-snippets"
			},
			{
				"language": "json",
				"path": "./snippets/prettier.code-snippets"
			},
			{
				"language": "json",
				"path": "./snippets/stylelint.code-snippets"
			},
			{
				"path": "./snippets/webpack.code-snippets"
			},
			{
				"language": "json",
				"path": "./snippets/settings.code-snippets"
			}
		],
		"keybindings": [
			{
				"command": "vscgettext.moveToNextUntranslated",
				"key": "alt+n",
				"when": "editorTextFocus && resourceLangId == po"
			},
			{
				"command": "vscgettext.moveToPreviousUntranslated",
				"key": "alt+shift+n",
				"when": "editorTextFocus && resourceLangId == po"
			}
		],
		"configuration": []
	},
	"devDependencies": {
		"prettier": "^2.5.1"
	}
}
