{
	"name": "vscode-panda-utils",
	"displayName": "Panda Utils",
	"description": "Tools picked from various extensions",
	"version": "0.7.0",
	"publisher": "EdwinKofler",
	"engines": {
		"vscode": "^1.12.0"
	},
	"license": "BSD-3-Clause",
	"repository": {
		"type": "git",
		"url": "https://github.com/hyperupcall/vscode-panda-utils"
	},
	"bugs": {
		"url": "https://github.com/hyperupcall/vscode-panda-utils/issues"
	},
	"author": {
		"name": "Edwin Kofler",
		"url": "https://edwinkofler.com"
	},
	"categories": [
		"Snippets"
	],
	"scripts": {},
	"icon": "images/icon.jpg",
	"contributes": {
		"languages": [
			{
				"id": "dircolors",
				"aliases": [
					"dir_colors"
				],
				"extensions": [
					".dircolors",
					".dir_colors"
				],
				"configuration": "./language-configuration/dircolors.language-configuration.json"
			},
			{
				"id": "i3",
				"aliases": [
					"i3_config"
				],
				"extensions": [
					".i3.config"
				],
				"configuration": "./language-configuration/i3.language-configuration.json"
			},
			{
				"id": "ninja",
				"aliases": [
					"Ninja"
				],
				"extensions": [
					".ninja"
				],
				"configuration": "./language-configuration/ninja.language-configuration.json"
			},
			{
				"id": "sxhkdrc",
				"aliases": [
					"sxhkdrc"
				],
				"extensions": [
					".sxhkdrc.conf"
				],
				"configuration": "./language-configuration/sxhkdrc.language-configuration.json"
			},
			{
				"id": "terminfo",
				"aliases": [
					"termcap"
				],
				"extensions": [
					".termcap",
					".src"
				],
				"configuration": "./language-configuration/terminfo.language-configuration.json"
			},
			{
				"id": "xdefaults",
				"aliases": [
					"Xdefaults",
					"xdefaults"
				],
				"extensions": [
					".Xresources",
					".xresources",
					".Xdefaults",
					".xdefaults"
				],
				"configuration": "./language-configuration/xresources.language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "dircolors",
				"scopeName": "source.dircolors",
				"path": "./syntaxes/dircolors.tmLanguage.json"
			},
			{
				"language": "i3",
				"scopeName": "source.i3",
				"path": "./syntaxes/i3.tmLanguage.json"
			},
			{
				"language": "ninja",
				"scopeName": "source.ninja",
				"path": "./syntaxes/ninja.tmLanguage.json"
			},
			{
				"language": "sxhkdrc",
				"scopeName": "source.sxhkdrc",
				"path": "./syntaxes/sxhkdrc.tmLanguage.json"
			},
			{
				"language": "terminfo",
				"scopeName": "source.terminfo",
				"path": "./syntaxes/terminfo.tmLanguage.json"
			},
			{
				"language": "xdefaults",
				"scopeName": "source.xdefaults",
				"path": "./syntaxes/xdefaults.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"path": "./snippets/basalt.code-snippets"
			},
			{
				"path": "./snippets/editorconfig.code-snippets"
			},
			{
				"path": "./snippets/eslint.code-snippets"
			},
			{
				"path": "./snippets/prettier.code-snippets"
			},
			{
				"path": "./snippets/stylelint.code-snippets"
			},
			{
				"path": "./snippets/webpack.code-snippets"
			},
			{
				"path": "./snippets/settings.code-snippets"
			}
		]
	},
	"devDependencies": {
		"prettier": "^2.5.1"
	}
}
